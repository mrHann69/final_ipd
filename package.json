{
  "name": "bookingapp",
  "version": "1.0.0",
  "description": "aplicacion para la reserva de hotel y vuelo",
  "main": "src/index.js",
  "scripts": {
    "start": "node --trace-warnings src/index.js",
    "dev": "nodemon --trace-warnings src/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",

    "dbhotel:create": "env-cmd -e devhotel sequelize-cli db:create",
    "migrations-devhotel:run": "env-cmd -e devhotel sequelize-cli db:migrate",
    "migrations-devhotel:delete": "env-cmd -e devhotel sequelize-cli db:migrate:undo:all",
    "migrations-devhotel:revert": "env-cmd -e devhotel sequelize-cli db:migrate:undo",

    "dbflight:create": "env-cmd -e devflight sequelize-cli db:create",
    "migrations-devflight:run": "env-cmd -e devflight sequelize-cli db:migrate",
    "migrations-devflight:delete": "env-cmd -e devflight sequelize-cli db:migrate:undo:all",
    "migrations-devflight:revert": "env-cmd -e devflight sequelize-cli db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrHann69/final_ipd.git"
  },
  "author": "mrHann69",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrHann69/final_ipd/issues"
  },
  "homepage": "https://github.com/mrHann69/final_ipd#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.3.4",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1"
  },
  "type": "commonjs"
}
